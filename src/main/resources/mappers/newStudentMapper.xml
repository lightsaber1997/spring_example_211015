<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.example.lesson04.dao.StudentDAO">
 	<insert id="insertStudent" parameterType="com.example.lesson04.model.Student" useGeneratedKeys="true" keyProperty="id">
 		insert into new_student
 		(
			name,
			phoneNumber,
			email,
			dreamJob
 		)
 		values
 		(
		#{name},
		#{phoneNumber},
		#{email},
		#{dreamJob}
 		)
 	</insert>
 	
 	<select id="selectStudentById" parameterType="int" resultType="com.example.lesson04.model.Student">
 		select
 			*
 		from
 			`new_student`
 		where 
 			id = #{id}
 	</select>
 </mapper>